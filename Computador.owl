                                         [ rdf:type owl:Restriction ;
                                           owl:onProperty :tiene_Parte ;
                                           owl:someValuesFrom :Aire
                                         ] ,
                                         [ rdf:type owl:Restriction ;
                                           owl:onProperty :tiene_Parte ;
                                           owl:someValuesFrom :Dual_Core
                                         ] ,
                                         [ rdf:type owl:Restriction ;
                                           owl:onProperty :tiene_Parte ;
                                           owl:someValuesFrom :HDD
                                         ] ,
                                         [ rdf:type owl:Restriction ;
                                           owl:onProperty :tiene_Parte ;
                                           owl:someValuesFrom :Micro-ATX
                                         ] ,
                                         [ rdf:type owl:Restriction ;
                                           owl:onProperty :tiene_Parte ;
                                           owl:allValuesFrom :Integrada
                                         ] ;
                         rdfs:comment "Acer Chromebook 15CB315"^^xsd:string ;
                         rdfs:label "Acer_Chromebook_15CB315"^^xsd:string .


###  http://www.computador.org#Acer_Nitro5
:Acer_Nitro5 rdf:type owl:Class ;
             rdfs:subClassOf :Disenos_Computadores ,
                             [ rdf:type owl:Restriction ;
                               owl:onProperty :tiene_Parte ;
                               owl:someValuesFrom :Micro-ATX
                             ] ,
                             [ rdf:type owl:Restriction ;
                               owl:onProperty :tiene_Parte ;
                               owl:someValuesFrom :Quad_Core
                             ] ,
                             [ rdf:type owl:Restriction ;
                               owl:onProperty :tiene_Parte ;
                               owl:someValuesFrom [ rdf:type owl:Class ;
                                                    owl:unionOf ( :HDD
                                                                  :SSD
                                                                )
                                                  ]
                             ] ,
                             [ rdf:type owl:Restriction ;
                               owl:onProperty :tiene_Gabinete ;
                               owl:allValuesFrom :Portatil
                             ] ,
                             [ rdf:type owl:Restriction ;
                               owl:onProperty :tiene_Parte ;
                               owl:allValuesFrom :Aire
                             ] ,
                             [ rdf:type owl:Restriction ;
                               owl:onProperty :tiene_Parte ;
                               owl:allValuesFrom :Dedicada
                             ] ;
