@prefix : <http://www.computador.org#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@base <http://www.computador.org> .

<http://www.computador.org> rdf:type owl:Ontology ;
                             owl:imports <http://sensormeasurement.appspot.com/ont/home/homeActivity#> .

#################################################################
#    Object Properties
#################################################################

###  http://www.computador.org#es_Componente_De
:es_Componente_De rdf:type owl:ObjectProperty ;
                  owl:inverseOf :tiene_Componente ;
                  rdfs:comment "El componente es parte de otra parte"^^xsd:string ;
                  rdfs:label "es_Componente_De"^^xsd:string .


###  http://www.computador.org#es_Gabinete_De
:es_Gabinete_De rdf:type owl:ObjectProperty ;
                rdfs:subPropertyOf :es_Componente_De ;
                rdfs:comment "El gabinete es de otra parte"^^xsd:string ;
                rdfs:label "es_Gabinete_De"^^xsd:string .


###  http://www.computador.org#es_Parte_De
:es_Parte_De rdf:type owl:ObjectProperty ;
             rdfs:subPropertyOf :es_Componente_De ;
             rdfs:comment "Es de parte"^^xsd:string ;
             rdfs:label "es_Parte_De"^^xsd:string .


###  http://www.computador.org#tiene_Ancestro
:tiene_Ancestro rdf:type owl:ObjectProperty ;
                rdfs:subPropertyOf :tiene_Coincidencia ;
                rdf:type owl:TransitiveProperty ;
                rdfs:comment "Existe un computador anterior a él"^^xsd:string ;
                rdfs:label "tiene_Ancestro"^^xsd:string .


###  http://www.computador.org#tiene_Coincidencia
:tiene_Coincidencia rdf:type owl:ObjectProperty ,
                             owl:SymmetricProperty ;
                    rdfs:comment "Tiene alguna relación"^^xsd:string ;
                    rdfs:label "tiene_Coincidencia"^^xsd:string .


###  http://www.computador.org#tiene_Componente
:tiene_Componente rdf:type owl:ObjectProperty ;
                  rdfs:comment "Un objeto tiene un componente"^^xsd:string ;
                  rdfs:label "tiene_Componente"^^xsd:string .


###  http://www.computador.org#tiene_Gabinete
:tiene_Gabinete rdf:type owl:ObjectProperty ;
                rdfs:subPropertyOf :tiene_Componente ;
                rdf:type owl:FunctionalProperty ;
                rdfs:domain :Computador ;
                rdfs:range :Gabinete ;
                rdfs:comment "Un objeto tiene gabinete"^^xsd:string ;
                rdfs:label "tiene_Gabinete"^^xsd:string .


###  http://www.computador.org#tiene_Generacion
:tiene_Generacion rdf:type owl:ObjectProperty ;
                  rdfs:comment "Tiene un computador anterior, pero será usado para ver si tiene un computador posterior a él o un computador en segunda generación o tercera generación"^^xsd:string ;
                  rdfs:label "tiene_Generacion"^^xsd:string .


###  http://www.computador.org#tiene_Hijo_Generacion
:tiene_Hijo_Generacion rdf:type owl:ObjectProperty ;
                       rdfs:subPropertyOf :tiene_Generacion ;
                       rdf:type owl:InverseFunctionalProperty ;
                       rdfs:comment "Existe un computador posterior a él"^^xsd:string ;
                       rdfs:label "tiene_Hijo_Generacion"^^xsd:string .


###  http://www.computador.org#tiene_Parte
:tiene_Parte rdf:type owl:ObjectProperty ;
             rdfs:subPropertyOf :tiene_Componente ;
             rdfs:domain :Computador ;
             rdfs:range :Parte_Computador ;
             rdfs:comment "Un objeto tiene parte"^^xsd:string ;
             rdfs:label "tiene_Parte"^^xsd:string .


###  http://www.computador.org#tiene_Segunda_Generacion
:tiene_Segunda_Generacion rdf:type owl:ObjectProperty ;
                          rdfs:subPropertyOf :tiene_Generacion ;
                          owl:propertyChainAxiom ( :tiene_Ancestro
                                                   :tiene_Ancestro
                                                 ) ;
                          rdfs:comment "Tiene un computador anterior a su primera línea anterior"^^xsd:string ;
                          rdfs:label "tiene_Segunda_Generacion"^^xsd:string .


###  http://www.computador.org#tiene_Tercera_Generacion
:tiene_Tercera_Generacion rdf:type owl:ObjectProperty ;
                          rdfs:subPropertyOf :tiene_Generacion ;
                          owl:propertyChainAxiom ( :tiene_Segunda_Generacion
                                                   :tiene_Ancestro
                                                 ) ;
                          rdfs:comment "Tiene un computador anterior a segunda generación"^^xsd:string ;
                          rdfs:label "tiene_Tercera_Generacion"^^xsd:string .


#################################################################
#    Data properties
#################################################################

###  http://www.computador.org#tiene_Precio
:tiene_Precio rdf:type owl:DatatypeProperty ;
              rdfs:domain :Computador ;
              rdfs:range xsd:int ;
              rdfs:comment "Contiene un precio"^^xsd:string ;
              rdfs:label "tiene_Precio"^^xsd:string .


#################################################################
#    Classes
#################################################################

###  http://www.computador.org#ATX
:ATX rdf:type owl:Class ;
