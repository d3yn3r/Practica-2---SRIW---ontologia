###  http://www.computador.org#Computador
:Computador rdf:type owl:Class ;
            rdfs:subClassOf [ rdf:type owl:Restriction ;
                              owl:onProperty :tiene_Gabinete ;
                              owl:someValuesFrom :Gabinete
                            ] ,
                            [ rdf:type owl:Restriction ;
                              owl:onProperty :tiene_Precio ;
                              owl:someValuesFrom xsd:integer
                            ] ;
            rdfs:comment "Computadora común y corriente, todo dispositivo eléctronico capaz de realizar operaciones lógicas y matematicas"^^xsd:string ;
            rdfs:label "Computador"^^xsd:string .


###  http://www.computador.org#Computador_Antiguo
:Computador_Antiguo rdf:type owl:Class ;
                    owl:equivalentClass [ owl:intersectionOf ( :Computador
                                                               [ rdf:type owl:Restriction ;
                                                                 owl:onProperty :tiene_Parte ;
                                                                 owl:someValuesFrom :Single_Core
                                                               ]
                                                             ) ;
                                          rdf:type owl:Class
                                        ] ;
                    rdfs:subClassOf :Computador ;
                    rdfs:comment "Un computador que sea lo demasiado lento hasta para los estandares actuales, tienden a ser aquellos antes del 2000"^^xsd:string ;
                    rdfs:label "Computador_Antiguo"^^xsd:string .


###  http://www.computador.org#Computador_Bajo_Rendimiento
:Computador_Bajo_Rendimiento rdf:type owl:Class ;
                             owl:equivalentClass [ owl:intersectionOf ( :Computador
                                                                        [ rdf:type owl:Class ;
                                                                          owl:complementOf :Computador_Gamer
                                                                        ]
                                                                        [ rdf:type owl:Restriction ;
                                                                          owl:onProperty :tiene_Parte ;
                                                                          owl:maxQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                                                          owl:onClass :Disco_Duro
                                                                        ]
                                                                      ) ;
                                                   rdf:type owl:Class
                                                 ] ;
                             rdfs:subClassOf :Computador ;
                             rdfs:comment "Como dice la clase, un computador que no alcanza los estandares de velocidad actuales"^^xsd:string ;
                             rdfs:label "Computador_Bajo_Rendimiento"^^xsd:string .


###  http://www.computador.org#Computador_Gamer
:Computador_Gamer rdf:type owl:Class ;
                  owl:equivalentClass [ owl:intersectionOf ( :Computador
                                                             [ rdf:type owl:Class ;
                                                               owl:unionOf ( [ rdf:type owl:Restriction ;
                                                                               owl:onProperty :tiene_Parte ;
                                                                               owl:someValuesFrom :Dedicada
                                                                             ]
                                                                             [ rdf:type owl:Restriction ;
                                                                               owl:onProperty :tiene_Parte ;
                                                                               owl:someValuesFrom :Octa_Core
                                                                             ]
                                                                           )
                                                             ]
                                                           ) ;
                                        rdf:type owl:Class
                                      ] ;
                  rdfs:subClassOf :Computador .


###  http://www.computador.org#Computador_Moderno
:Computador_Moderno rdf:type owl:Class ;
                    owl:equivalentClass [ owl:intersectionOf ( :Computador
                                                               [ rdf:type owl:Class ;
                                                                 owl:complementOf :Computador_Antiguo
                                                               ]
                                                               [ rdf:type owl:Restriction ;
                                                                 owl:onProperty :tiene_Parte ;
                                                                 owl:maxQualifiedCardinality "8"^^xsd:nonNegativeInteger ;
                                                                 owl:onClass :Disco_Duro
                                                               ]
                                                             ) ;
                                          rdf:type owl:Class
                                        ] ;
                    rdfs:subClassOf :Computador ;
                    rdfs:comment "Computadora que tiende a ser después de los 2000"^^xsd:string ;
                    rdfs:label "Computador_Moderno"^^xsd:string .


###  http://www.computador.org#Computador_Oficina
:Computador_Oficina rdf:type owl:Class ;
                    owl:equivalentClass [ owl:intersectionOf ( :Computador
