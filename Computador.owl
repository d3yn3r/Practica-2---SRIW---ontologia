                                                                 owl:onProperty :tiene_Parte ;
                                                                 owl:someValuesFrom :Integrada
                                                               ]
                                                             ) ;
                                          rdf:type owl:Class
                                        ] ;
                    rdfs:subClassOf :Computador ;
                    rdfs:comment "Computadora especializada en los trabajos de oficina"^^xsd:string ;
                    rdfs:label "Computador_Oficina"^^xsd:string .


